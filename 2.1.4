import turtle

# Create a new turtle screen and set its background color
screen = turtle.Screen()
screen.bgcolor("white")

# Create a new turtle named "tracy"
tracy = turtle.Turtle()

# Function to draw a circle and move forward
def draw_circle_and_move(turtle, radius, distance):
    turtle.circle(radius)
    turtle.penup()
    turtle.forward(distance)
    turtle.pendown()

